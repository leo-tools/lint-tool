.err_page {
    width: 100vw;
    height: 100vh;
    position: absolute;
    background: no-repeat center;
    background-size: 100%;
    background: white;
}

.err_600 {
    width: 490rpx;
    height: 466rpx;
    position: absolute;
    background: no-repeat center;
    background-size: 100%;
    top: 50%;
    left: 50%;
    transform: translateY(-350rpx) translateX(-50%);
}

