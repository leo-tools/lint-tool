.logo_error{
  display:flex;
  justify-content: center;
}
.logo_error>view{
  position:absolute;
  top:27%;
  display:flex;
  flex-direction: column;
  align-items: center;
}
.logo_error image{
  width:400rpx;
}
.logo_error .text{
  width:280rpx;
  font-size:28rpx;
  line-height:44rpx;
  margin-top:32rpx;
  color:#A0A0A0;
  text-align: center;
}
